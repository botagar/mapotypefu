{
  "name": "mapotypefu",
  "version": "2.0.0",
  "description": "A TypeScript library wrapper for Open Tofu CLI, providing type-safe infrastructure as code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.cjs",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "pnpm run build",
    "preversion": "pnpm run lint",
    "version": "pnpm run format && git add -A src"
  },
  "keywords": [
    "opentofu",
    "terraform",
    "typescript",
    "infrastructure",
    "iac",
    "cloud"
  ],
  "author": "botagar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botagar/mapotypefu.git"
  },
  "bugs": {
    "url": "https://github.com/botagar/mapotypefu/issues"
  },
  "homepage": "https://github.com/botagar/mapotypefu#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "docs/**/*"
  ],
  "dependencies": {
    "execa": "^9.6.0"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
