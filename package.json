{
  "name": "mapotypefu",
  "version": "1.1.0",
  "description": "A TypeScript library wrapper for Open Tofu CLI, providing type-safe infrastructure as code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.cjs",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm test && pnpm run lint",
    "preversion": "pnpm run lint",
    "version": "pnpm run format && git add -A src"
  },
  "keywords": [
    "opentofu",
    "terraform",
    "typescript",
    "infrastructure",
    "iac",
    "cloud"
  ],
  "author": "botagar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botagar/mapotypefu.git"
  },
  "bugs": {
    "url": "https://github.com/botagar/mapotypefu/issues"
  },
  "homepage": "https://github.com/botagar/mapotypefu#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "semantic-release": "^24.2.6",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "docs/**/*"
  ],
  "dependencies": {
    "execa": "^7.1.1"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
